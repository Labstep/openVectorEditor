{
  "name": "@teselagen/apollo-methods",
  "version": "1.2.2",
  "private": false,
  "description": "Apollo helper methods for working with a react-apollo graphql application",
  "main": "lib/index.js",
  "author": "TeselaGen",
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest",
    "auto-cover": "jest --coverage --watch",
    "auto-test": "jest --watch",
    "cover": "jest --coverage",
    "prepare": "yarn build",
    "build": "nwb build-react-component --copy-files --no-demo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --rule 'import/order: 1' --rule 'react/jsx-curly-brace-presence: 1' --fix --max-warnings=0",
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "dependencies": {
    "pascal-case": "^2.0.1"
  },
  "devDependencies": {
    "apollo-client": "^2.5.1",
    "babel-eslint": "^8.2.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-teselagen": "^4.1.0",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-no-only-tests": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "jest-summary-reporter": "^0.0.2",
    "lint-staged": "^8.0.4",
    "lodash": "^4.17.11",
    "nwb": "^0.23.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.8.0",
    "react": "^16.8.3",
    "react-apollo": "^2.5.1",
    "react-dom": "^16.8.3",
    "recompose": "^0.30.0",
    "uniqid": "^4.1.1"
  },
  "peerDependencies": {
    "apollo-client": "^2.5.1",
    "graphql": "*",
    "graphql-tag": "^2.8.0",
    "lodash": "*",
    "react-apollo": "*",
    "recompose": "*",
    "uniqid": "*"
  }
}
