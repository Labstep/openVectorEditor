{
    "type": "root",
    "entity": "Author",
    "filters": [
        {
            "type": "group",
            "operator": "and",
            "filters": [
                {
                    "type": "where",
                    "args": {
                        "firstName": "Adam",
                        "lastName": "Thomas"
                    }
                },
                {
                    "type": "where",
                    "args": {
                        "city": "Carmel"
                    }
                },
                {
                    "type": "group",
                    "operator": "and",
                    "filters": [
                        {
                            "type": "subquery",
                            "key": "Author_id",
                            "entity": "Post",
                            "foreignKey": "id",
                            "filters": [
                                {
                                    "type": "group",
                                    "operator": "and",
                                    "filters": [
                                        {
                                            "type": "expression",
                                            "operator": "greaterThan",
                                            "field": "likes",
                                            "args": [
                                                10
                                            ]
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "type": "subquery",
                            "key": "id",
                            "entity": "Group",
                            "foreignKey": "Group_id",
                            "filters": [
                                {
                                    "type": "group",
                                    "operator": "and",
                                    "filters": [
                                        {
                                            "type": "expression",
                                            "operator": "inList",
                                            "field": "groupType",
                                            "args": [
                                                [
                                                    "admin",
                                                    "supervisor"
                                                ]
                                            ]
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "subquery",
                    "key": "Author_id",
                    "entity": "AuthorRole",
                    "foreignKey": "id",
                    "filters": [
                        {
                            "type": "group",
                            "operator": "and",
                            "filters": [
                                {
                                    "type": "subquery",
                                    "key": "id",
                                    "entity": "Role",
                                    "foreignKey": "Role_id",
                                    "filters": [
                                        {
                                            "type": "group",
                                            "operator": "and",
                                            "filters": [
                                                {
                                                    "type": "where",
                                                    "args": {
                                                        "name": "Super-Author"
                                                    }
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "subquery",
                    "key": "code",
                    "entity": "AuthorType",
                    "foreignKey": "AuthorType_code",
                    "modifier": "not",
                    "filters": [
                        {
                            "type": "group",
                            "operator": "and",
                            "filters": [
                                {
                                    "type": "where",
                                    "args": {
                                        "name": "Retired"
                                    }
                                },
                                {
                                    "type": "where",
                                    "args": {
                                        "name": "Deceased"
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "group",
            "operator": "or",
            "chainedWith": "and",
            "filters": [
                {
                    "type": "where",
                    "args": {
                        "state": "IN",
                        "country": "USA"
                    }
                },
                {
                    "type": "where",
                    "args": {
                        "zipCode": "46032"
                    }
                },
                {
                    "type": "expression",
                    "operator": "greaterThan",
                    "field": "age",
                    "args": [
                        30
                    ]
                }
            ]
        }
    ]
}